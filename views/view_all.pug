extends ../layout

block content
    h3 Students
    a(href='/courses/add') Add New Student
    table
        thead
        tr
        th First
        th Last
        th Grade
        th
        tbody
            each course in students
                tr
                td= student.first_name
                td= student.last_name
                td= student.grade_level
                td
                  a(href=`/student/profile/${student.id}`) View Student